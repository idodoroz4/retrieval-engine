        mc:Ignorable="d"
        Title="show_memory" Height="300" Width="300">
    <Grid Margin="-233,-98,0.333,-0.333">
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox x:Name="memory_txt" HorizontalAlignment="Left" Height="298" Margin="230,76,-3,-5.333" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="299" RenderTransformOrigin="0.5,0.5" Grid.RowSpan="2">
