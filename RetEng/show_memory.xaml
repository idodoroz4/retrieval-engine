<Window x:Class="RetEng.show_memory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RetEng"
        mc:Ignorable="d"
        Title="show_memory" Height="300" Width="300">
    <Grid Margin="-233,-98,0.2,-0.2" RenderTransformOrigin="0.352,0.424">
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <RichTextBox x:Name="memory_txt" HorizontalAlignment="Left" Height="294" Margin="230,77,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="288">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
